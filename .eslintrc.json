{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "no-useless-escape": "off",
    "consistent-return": "off",
    "func-names": "off",
    "prefer-arrow-callback": "off",
    "max-len": "off",
    "linebreak-style": ["warn", "windows"]
  }
}
